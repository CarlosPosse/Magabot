<launch>
  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
		<param name="base_local_planner" value="base_local_planner/TrajectoryPlannerROS" />
    <rosparam file="$(find mag_window)/src/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find mag_window)/src/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find mag_window)/src/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find mag_window)/src/global_costmap_params.yaml" command="load" />
    <rosparam file="$(find mag_window)/src/base_local_planner_params.yaml" command="load" />
		<rosparam file="$(find mag_window)/src/move_base_params.yaml" command="load" />
		<remap from="odom" to="odom_combined"/>
  </node>

	<!--<node pkg="mag_window" type="Footprint" name="global_footprint">
		<remap from="footprint" to="move_base/global_costmap/footprint"/>
	</node>

	<node pkg="mag_window" type="Footprint" name="local_footprint">
		<remap from="footprint" to="move_base/local_costmap/footprint"/>
	</node>-->
</launch>
